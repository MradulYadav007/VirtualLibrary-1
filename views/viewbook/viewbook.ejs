<table>
    <thead>
        <tr>
        <th>Book Name</th>
        <th>Catagory</th>
        <th>Page No.</th>
        <th>Subscribe</th>
        </tr>
    </thead>
    <tbody>
        
        <% books.forEach(book => { %>
            <tr>
                <td><%= book.title %> </td>
                <td><%= book.category %></td>
                <td><%= book.pageCount %></td>
                
                <td><button class="btn-outline-success" type="button" id="subscribe"><a href="/viewbook/type2/<%= book._id %> ">Subscribe</a></button></td>
                </tr>
        <%}) %> 

        

    </tbody>
</table>


    
